{
    "name": "tencentrapidstudio",
    "displayName": "rapidstudio",
    "description": "Visual Studio Code Extension for Tencent Rapidview",
    "version": "0.0.1",
    "publisher": "realhe",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.syncFile",
        "onCommand:extension.syncProject",
        "workspaceContains:rapid_workspace.json"        
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.syncFile",
                "title": "Sync File"
            },
            {
                "command": "extension.syncProject",
                "title": "Sync Project"
            },
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "extension.syncFile",
                    "group": "navigation"
                },
                {
                    "command": "extension.syncProject",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "xml2js": "^0.4.19"
    }
}
